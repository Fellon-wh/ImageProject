<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.feilong.spider.image.vo.Image" table="images">
		<id name="id">
			<generator class="native"/>
		</id>
		<property name="src"/>
		<property name="name"/>
		<property name="path"/>
		<property name="uid"/>
	</class>
</hibernate-mapping>